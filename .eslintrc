{
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@babel/eslint-parser",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "curly": "warn",
    "quotes": ["warn", "single"],
    "linebreak-style": "off",
    "no-console": "warn",
    "no-warning-comments": ["warn", { "terms": ["todo", "fixme"]}]
  }
}
